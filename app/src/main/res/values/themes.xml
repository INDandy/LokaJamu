<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyBottomNavTest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:itemTextAppearance">@style/CustomTextItems</item>
    </style>

    <style name="CustomTextItems" parent="@android:style/TextAppearance.Widget.IconMenu.Item">
        <item name="fontFamily">sans-serif-light</item>
    </style>
    <style name="AppTheme" parent="Theme.Material3.Light.NoActionBar">
        <item  name="android:windowActionBar">@color/hijau</item>
        <item name="android:windowBackground">@color/hijau</item>

    </style>
</resources>